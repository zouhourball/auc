{
  "name": "target-fe-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate of front end.",
  "repository": "https://gitlab.com/target-digital-transformation/target-fe-boilerplate.git",
  "author": "Target Energy Solution",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "run-s dev",
    "dev": "cross-env BUILD_PROFILE_NAME=dev NODE_ENV=development BABEL_ENV=development NODE_OPTIONS='--max-old-space-size=8000' webpack-dev-server --config webpack.dev.js",
    "debug": "cross-env NODE_ENV=development webpack-dev-server --config webpack.debug.js",
    "build": "run-s lint build:no-lint",
    "analyze": "run-s lint clean build:analyze",
    "build:no-lint": "run-s clean build:webpack",
    "build:webpack": "cross-env NODE_ENV=production NODE_OPTIONS='--max-old-space-size=8000' webpack --config webpack.prod.js",
    "build:analyze": "cross-env NODE_ENV=production webpack --config webpack.analyze.js",
    "clean": "rimraf dist",
    "lint": "run-p lint:js lint:style",
    "lint:js": "eslint \"**/*.{j,t}s{,x}\"",
    "lint:style": "stylelint \"**/*.@(css|sass|scss)\"",
    "format": "run-p format:js format:jsx format:ts format:style format:others",
    "format:js": "prettier-eslint --write \"**/*.js\"",
    "format:jsx": "prettier-eslint --write \"**/*.jsx\"",
    "format:ts": "prettier-eslint --write \"**/*.ts\"",
    "format:style": "run-s format:style:prettier format:style:stylelint",
    "format:others": "prettier --write \"**/*.@(json|gql|graphql)\"",
    "format:style:prettier": "prettier --write \"**/*.@(css|sass|scss)\"",
    "format:style:stylelint": "stylelint \"**/*.@(css|sass|scss)\" --fix",
    "test": "jest --config ./jest.config.json",
    "test:watch": "jest --watchAll --config ./jest.config.json"
  },
  "lint-staged": {
    "**/*.@(js|jsx)": [
      "prettier-eslint --write",
      "eslint",
      "git add"
    ],
    "**/*.@(ts|tsx)": [
      "prettier-eslint --write",
      "eslint",
      "git add"
    ],
    "**/*.@(css|sass|scss)": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ],
    "**/*.@(json|gql|graphql)": [
      "prettier --write",
      "git add"
    ]
  },
